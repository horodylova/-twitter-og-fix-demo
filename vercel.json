{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/images/(.*)", "dest": "/images/$1" },         
    { "src": "/robots.txt", "dest": "/robots.txt" },        
    { "src": "/sitemap.xml", "dest": "/server.js" },        
    { "src": "/post/(.*)", "dest": "/server.js" },           
    { "src": "/", "dest": "/server.js" }                
  ]
}